      scope: constant.language.optex.builtin
    - include: primitives
    - include: optex_macros
    - include: macros_general
    - match: '\$'
      scope: storage.math
      pop: true

  brackets:
    - match: '\['
      push: bracket_parameter

  verbatim_begin:
    - match: '\\begtt'
      scope: constant.language.optex.builtin
      push: verbatim

  verbatim:
    - meta_content_scope: text.verbatim
    - match: '\\endtt'
      scope: constant.language.optex.builtin
      pop: true

  bracket_parameter:
    - meta_content_scope: variable.parameter #set the scope for the text inside brackets, but doesn't inlclude the brackets
    - match: '\]'
      pop: true