  macros_definition:
    - match: '(\\[eg]?def)\x20*(\\[[:alpha:]]+)'
      captures:
        1: keyword.optex.definition
        2: entity.name.user_macro.optex
      comment: User-defined macros following def/edef/gdef command

  macros_general:
    - match: '\\[[:alpha:]]+'
      scope: entity.name.general_macro.optex

  double_dollar:
    - match: '\$\$'
      scope: storage.math
      push: block_math

